{
  "private": true,
  "name": "@queue-run/server",
  "scripts": {
    "build:remix": "remix build",
    "build:tailwind": "tailwindcss -i app/styles/tailwind.css -o app/styles/tailwind.full.css",
    "build": "yarn build:tailwind && yarn build:remix",
    "dev:remix": "doppler run remix dev",
    "dev:tailwind": "tailwindcss -i app/styles/tailwind.css -o app/styles/tailwind.full.css --watch",
    "dev": "concurrently npm:dev:*",
    "postinstall": "remix setup node"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@aws-sdk/client-dynamodb": "^3.44.0",
    "@aws-sdk/client-sqs": "^3.43.0",
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@remix-run/react": "^1.0.6",
    "@remix-run/serve": "^1.0.6",
    "@remix-run/vercel": "^1.0.6",
    "antd": "^4.17.3",
    "date-fns": "^2.27.0",
    "dotenv": "^10.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.0.6",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.0.6",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.0",
    "@tailwindcss/typography": "^0.5.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^6.4.0",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.5",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": "14.x"
  },
  "sideEffects": false
}
