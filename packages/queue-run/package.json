{
  "name": "queue-run",
  "version": "0.3.0",
  "exports": {
    ".": "./dist/index.js",
    "./jsx-runtime": "./dist/index.js"
  },
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "queue-run": "./bin/cli.js"
  },
  "scripts": {
    "build": "rm -rf dist && yarn build:bundle --analyze && yarn build:types",
    "build:bundle": "yarn esbuild src/index.ts --bundle --platform=node --target=node14 --outfile=dist/index.js --format=esm --sourcemap --inject:./src/require_shim.js",
    "build:types": "yarn tsc -p tsconfig.json --emitDeclarationOnly",
    "lint": "yarn eslint src/**/*.ts",
    "prepare": "yarn build",
    "watch": "concurrently \"yarn build:bundle --watch\" \"yarn build:types --watch\""
  },
  "files": [
    "bin",
    "dist",
    "src",
    "README.md"
  ],
  "dependencies": {
    "chalk": "5.0.0",
    "fast-glob": "^3.2.8",
    "fetch-blob": "^3.1.3",
    "node-abort-controller": "^3.0.1",
    "node-fetch": "3.1.0",
    "parse-multipart-data": "^1.2.1",
    "path-to-regexp": "^6.2.0",
    "source-map-support": "^0.5.21",
    "tiny-invariant": "^1.2.0",
    "xmlbuilder": "^15.1.1"
  },
  "devDependencies": {
    "@types/node-fetch": "3.0.3",
    "@types/source-map-support": "^0.5.4",
    "concurrently": "^7.0.0"
  },
  "engines": {
    "node": "14.x"
  },
  "gitHead": "cb2455561449ed3629e679e3656bb72e746096e3"
}
