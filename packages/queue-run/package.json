    "node-fetch": "3.1.0",
    "parse-multipart-data": "^1.2.1",
    "path-to-regexp": "^6.2.0",
    "source-map-support": "^0.5.21",
    "tiny-invariant": "^1.2.0",
    "xmlbuilder": "^15.1.1"
  },
  "engines": {
