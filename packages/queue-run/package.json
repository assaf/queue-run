    "filesize": "^8.0.7",
    "jws": "^4.0.0",
    "node-abort-controller": "^3.0.1",
    "node-fetch": "3.2.4",
    "path-to-regexp": "^6.2.1",
    "source-map-support": "^0.5.21",
    "tiny-invariant": "^1.2.0",
    "uuid": "^8.3.2",
    "xmlbuilder": "^15.1.1"
  },
  "devDependencies": {
