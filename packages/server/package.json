{
  "private": true,
  "name": "@queue-run/server",
  "scripts": {
    "build:remix": "remix build",
    "build:tailwind": "tailwindcss -i app/styles/tailwind.css -o app/styles/tailwind.full.css",
    "build": "yarn build:tailwind && yarn build:remix",
    "dev:remix": "doppler run remix dev",
    "dev:tailwind": "tailwindcss -i app/styles/tailwind.css -o app/styles/tailwind.full.css --watch",
    "dev": "concurrently npm:dev:*",
    "postinstall": "remix setup node"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.44.0",
    "@remix-run/react": "^1.0.6",
    "@remix-run/serve": "^1.0.6",
    "@remix-run/vercel": "^1.0.6",
    "dotenv": "^10.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.0.6",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.0.6",
    "@tailwindcss/forms": "^0.4.0-alpha.2",
    "@tailwindcss/line-clamp": "^0.2.2",
    "@tailwindcss/typography": "^0.5.0-alpha.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^6.4.0",
    "postcss": "^8.4.4",
    "tailwindcss": "^3.0.0-alpha.2",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": "14.x"
  },
  "sideEffects": false
}
