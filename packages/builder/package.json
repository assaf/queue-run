{
  "name": "queue-run-builder",
  "version": "0.13.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "yarn clean && yarn build:dist && yarn build:types",
    "build:dist": "yarn esbuild src/*.ts src/**/*.ts --platform=node --target=node14 --outdir=dist --format=esm --sourcemap",
    "build:types": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "lint": "yarn eslint src/**/*.ts",
    "prepare": "yarn build",
    "watch": "yarn clean && concurrently \"yarn build:dist --watch\" \"yarn build:types --watch\""
  },
  "files": [
    "dist",
    "types"
  ],
  "dependencies": {
    "@aws-sdk/client-acm": "^3.53.0",
    "@aws-sdk/client-apigatewayv2": "^3.53.0",
    "@aws-sdk/client-cloudwatch": "^3.53.0",
    "@aws-sdk/client-cloudwatch-events": "^3.53.0",
    "@aws-sdk/client-dynamodb": "^3.53.0",
    "@aws-sdk/client-iam": "^3.53.0",
    "@aws-sdk/client-lambda": "^3.53.0",
    "@aws-sdk/client-sqs": "^3.53.0",
    "@swc/core": "^1.2.146",
    "chalk": "^5.0.0",
    "cron-parser": "^4.2.1",
    "dotenv": "^16.0.0",
    "fast-glob": "^3.2.11",
    "filesize": "^8.0.7",
    "friendly-node-cron": "^1.4.1",
    "git-repo-info": "^2.1.1",
    "inquirer": "^8.2.0",
    "jszip": "^3.7.1",
    "lookpath": "^1.2.2",
    "node-abort-controller": "^3.0.1",
    "ora": "^6.1.0",
    "path-to-regexp": "^6.2.0",
    "queue-run": "^0.13.0",
    "queue-run-lambda": "^0.13.0",
    "semver": "^7.3.5",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@types/ms": "^0.7.31",
    "@types/semver": "^7.3.9"
  },
  "engines": {
    "node": "14.x"
  },
  "homepage": "https://queue.run",
  "gitHead": "8fd7f66958b6b030362b2d96946cfd804c015f70"
}
