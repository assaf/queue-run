    "src"
  ],
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.45.0",
    "queue-run": "^0.2.2",
    "tiny-invariant": "^1.2.0"
  },
  "engines": {
    "node": "14.x"
  },
